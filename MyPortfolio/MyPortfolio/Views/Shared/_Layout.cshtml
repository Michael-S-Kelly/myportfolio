
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="~/Libraries/Styles/normalize.css" />
    <link rel="stylesheet" href="~/Libraries/Styles/bootstrap.css" />
    <link rel="stylesheet" href="~/Styles/NavBar.css" />

    <script src="~/Libraries/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Libraries/Scripts/popper.min.js"></script>
    <script src="~/Libraries/Scripts/tooltip.min.js"></script>
    <script src="~/Libraries/Scripts/bootstrap.js"></script>

</head>
<body>
    <header>
        <nav class="navbar navbar-dark bg-dark navbar-expand-lg fixed-top">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a href="/Home" class="nav-link">About Me</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Featured Projects</a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" href="#">A Project</a>
                            <a class="dropdown-item" href="#">Another Project</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="#">Some Other Project</a>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link">Work Experience</a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="nav-link">School</a>
                    </li>
                </ul>
            </div>
            <a href="github.com/Michael-S-Kelly" class="nav-link"><img src="~/Images/icons/GitHub-Mark.png" id="githublogo" alt="Git Hub icon"/></a>
            <a href="linkedin.com/in/michael-s-kelly2019" class="nav-link"><img src="~/Images/icons/LinkedIn.png" id="linkedinlogo" alt="LinkedIn icon"/></a>
            <a href="#" class="nav-link"><img src="~/Images/icons/email.png" id="emaillogo" alt="Contact Me icon" /></a>
        </nav>
    </header>
    <main>
        @RenderBody()
    </main>
    <footer>

    </footer>
</body>
</html>
